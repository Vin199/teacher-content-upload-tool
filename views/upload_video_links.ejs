<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Upload Video Links</title>
    <link rel="stylesheet" href="/assets/css/uploadVideo.css">
</head>

<body>
    <div class="assessment-top-container">
        <img class="iPrep-img" src="/assets/images/dashboard_screen_logo.svg" alt="iPrep Logo" />
        <hr class="hrule-1">
        <div class="div-2">
            <a class="active" href="videos">
                <img src="/assets/images/arrow_back.svg" alt="arrow-back" /></a>
            <span class="create-assessment-text">Upload Video Links</span>
        </div>
        <div class="hr"></div>
    </div>
    <div class="vid-set-ques">
        <!-- <span class="question_text">Video Links</span> -->
        <div id="video_form">
            <div class="vid-ques-forms" id="question_form_1">
                <div class="vid-ques-area">
                    <span class="vid-question-num">1.</span>
                    <input type="text" name="videoName" class="video-name" placeholder="Video Name">
                    <input type="text" class="vid-link" name="videoLink" placeholder="Video Link">
                </div>
            </div>
        </div>
    </div>
    <div class="vid-btn-div">
        <button id="nextQues" onclick="add_more_video_field()" class="next-link">
            +
        </button>
        <span class="next-btn-text">Add More Questions</span>
        <input type="submit" id="videoSubmit">
    </div>
    <script src="/assets/scripts/upload_video_links.js"></script>
</body>

</html>